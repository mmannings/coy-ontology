    <!-- https://schema.coypu.org/#hasCustomer -->

    <owl:ObjectProperty rdf:about="https://schema.coypu.org/#hasCustomer">
        <owl:inverseOf rdf:resource="https://schema.coypu.org/#isCustomerOf"/>
        <rdfs:domain rdf:resource="https://schema.coypu.org/#Company"/>
        <rdfs:range rdf:resource="https://schema.coypu.org/#Company"/>
        <rdfs:comment xml:lang="en">the company providing a product, material, resource or service to another company</rdfs:comment>
        <rdfs:label xml:lang="en">hasCustomer</rdfs:label>
    </owl:ObjectProperty>
    


