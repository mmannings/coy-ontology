@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix coy: <https://schema.coypu.org/global#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix tiva: <https://schema.coypu.org/tiva#> .
@prefix fibo: <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/ClassificationSchemes/> .

# Remove file apsolute path and use only file name to run RDFizer.

<TriplesMap1>	
	rml:logicalSource [ rml:source "fdva_bsci_2018_updated.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "https://schema.coypu.org/global#{country_c}_{industry_code_i}_{country_p}_{industry_code_h}_{year}_{sum}";
		rr:class tiva:FdVaBsci;
	];		
	rr:predicateObjectMap [
		rr:predicate tiva:hasValueAddedOrigin;
		rr:objectMap [
			rr:template "https://schema.coypu.org/global#{country_c}_{industry_code_i}_{country_p}_{industry_code_h}_{year}_{sum}_value_added_origin";
			rr:class tiva:Vao;
		]
	];		
	rr:predicateObjectMap [
		rr:predicate tiva:hasFinalDemand;
		rr:objectMap [
			rr:template "https://schema.coypu.org/global#{country_c}_{industry_code_i}_{country_p}_{industry_code_h}_{year}_{sum}_final_demand";
			rr:class tiva:Fd ;
		]
	];
    rr:predicateObjectMap [
        rr:predicate coy:hasValue;
        rr:objectMap [
           rml:reference "sum";
           rr:datatype xsd:double;
      ]
    ];	
	rr:predicateObjectMap [
        rr:predicate coy:hasYear;
        rr:objectMap [
           rml:reference "year";
           rr:datatype xsd:dateTime;
      ]
    ].
